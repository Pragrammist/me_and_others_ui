fn icon_template_svg_path(
  height: String,
  width: String,
  view_box: String,
  class_names_div: String,
  class_names_svg: String,
  svg_path: String,
) {
  div([class(class_names_div)], [
    svg.svg(
      [
        attribute("viewBox", view_box),
        attribute("height", height),
        attribute("width", width),
      ],
      [
        svg.path([attribute("class", class_names_svg), attribute("d", svg_path)]),
      ],
    ),
  ])
}
